{
    "name": "nguyenhiep/cockroachdb-laravel",
    "description": "CockroachDB driver for Laravel 8",
    "keywords": ["cockroach", "cockroachdb", "laravel"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@cego.dk"
        },
        {
            "name": "Deon George",
            "email": "deon@leenooks.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nbj\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nbj\\Cockroach\\CockroachServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "illuminate/database": "^8.32",
        "illuminate/support": "^8.32"
    }
}
